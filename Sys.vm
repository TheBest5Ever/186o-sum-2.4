function Sys.min 0
    push argument 0
    push argument 1
    pop temp 1
    pop temp 0
    push temp 0
    push temp 1
    lt
    if-goto min
    goto notmin

label min
push argument 0 
return

label notmin
push argument 1
return


function Sys.max 0
	push argument 0
	push argument 1
	pop temp 1
	pop temp 0
	push temp 0
	push temp 1
	gt
	if-goto big
	goto small

label small
push argument 1
return

label big
push argument 0
return

function Sys.range 2
    push argument 0   
    pop pointer 1     
    push argument 1   
    pop temp 9        
    push constant 0
    pop temp 10       

label LOOP
    push temp 10
    push temp 9
    lt
    if-goto LOOP_BODY
    goto END

label LOOP_BODY
    push pointer 1
    push temp 10
    add
    pop pointer 0 

    push argument 0
    push temp 10
    call Sys.min 2
    pop temp 0

    push pointer 0
    push temp 0
    pop that 0

    push temp 10
    push constant 1
    add
    pop temp 10

    push pointer 1
    push constant 1
    add
    pop pointer 1
    goto LOOP

label END
    push argument 0 //push the address of the array.
    return