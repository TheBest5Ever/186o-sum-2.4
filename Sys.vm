function sys.min 0
    push argument 0
    push argument 1
    pop temp 1
    pop temp 0
    push temp 0
    push temp 1
    lt
    if-goto min
    goto notmin
label min
push argument 0 
return
label notmin
push argument 1
return


function Sys.max 0
	push argument 0
	push argument 1
	pop temp 1
	pop temp 0
	push temp 0
	push temp 1
	gt
	if-goto big
	goto small
label small
push argument 1
return
label big
push argument 0
return

function Sys.range 2
    push constant 0
    pop temp 10
    push argument 0 
    pop pointer 0
    push argument 1
    push constant 1
    gt
    not 
    if-goto zero
    push argument 1
    push constant 1
    sub
    pop temp 9
    push constant 16383
    pop temp 0
    push constant 16383
    neg
    pop temp 1
    goto LOOP

label zero
push constant 0
return

label LOOP
push temp 10
push temp 9
gt
if-goto END 
push this 0
push temp 0
call Sys.min 2
pop temp 0
push temp 0 
push this 0
eq
not 
if-goto MAXI
goto continue

label MAXI
push this 0 
push temp 1
call Sys.max 2
pop temp 1
goto continue 

label continue
push pointer 0
push constant 1
add
pop pointer 0
push temp 10
push constant 1
add
pop temp 10
goto LOOP
      
label END
push temp 1
push temp 0
sub
return

function Sys.init 0
    call Sys.range 2
label WHILE
    goto WHILE
