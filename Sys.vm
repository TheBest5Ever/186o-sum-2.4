function sys.min 0
    push argument 0
    push argument 1
    pop temp 1
    pop temp 0
    push temp 0
    push temp 1
    lt
    if-goto min
    goto notmin
label min
push argument 0 
return
label notmin
push argument 1
return


function Sys.max 0
	push argument 0
	push argument 1
	pop temp 1
	pop temp 0
	push temp 0
	push temp 1
	gt
	if-goto big
	goto small
label small
push argument 1
return
label big
push argument 0
return

function Sys.range 2
  push argument 1
  push constant 1
  gt
  not
  if-goto ZERO
  push argument 0
  pop pointer 0
  push argument 1
  push constant 1
  sub
  pop temp9
  push constant 16383
  pop temp2
  push constant 16383
  neg
  pop temp3
  push constant 0
  pop temp10
LABEL LOOP
  push temp10
  push temp9
  gt
  if-goto END
  push this 0
  push temp2
  call Sys.min 2
  pop temp2
  push temp2
  push this 0
  eq
  not
  if-goto NEW_MAX
  goto CONTINUE
LABEL NEW_MAX
  push this 0
  push temp3
  call Sys.max 2
  pop temp3
LABEL CONTINUE
  push pointer 0
  push constant 1
  add
  pop pointer 0
  push temp10
  push constant 1
  add
  pop temp10
  goto LOOP
LABEL END
  push temp3
  push temp2
  sub
  return
LABEL ZERO
  push constant 0
  return

function Sys.init 0
  call Sys.range 2
LABEL WHILE
  goto WHILE
